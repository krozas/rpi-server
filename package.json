{
  "name": "rpi-server",
  "version": "1.0.0",
  "description": "Server Rest Node para rpi",
  "engines": {
    "node": ">=20.x",
    "npm": ">=10.x"
  },
  "main": "dist/app.js",
  "scripts": {
    "build:ts": "npm run task:clean && npm run task-generate-prod",
    "build": "babel src --extensions \".ts,.js\" --out-dir dist --copy-files",
    "start": "node --env-file=.env pm2 start dist/app.js",
    "dev": "ts-node-dev --files -r tsconfig-paths/register  --env-file=.env --respawn --ignore-watch node_modules src/app.ts",
    "start:dev": "ts-node-dev -r tsconfig-paths/register  --inspect=0.0.0.0:9229 --env-file=.env --respawn --ignore-watch node_modules src/app.ts",
    "start:prod": "node --env-file=dist/.env.prod dist/app.js",
    "task:clean": "rimraf dist; exit 0",
    "task-generate-prod": "swc ./src -d dist",
    "clean:node_modules": "rimraf node_modules",
    "pm2:start": "pm2-runtime start ecosystem.config.js --env production",
    "pm2:stop": "pm2-runtime stop ecosystem.config.js",
    "pm2:restart": "pm2-runtime restart ecosystem.config.js",
    "pm2:delete": "pm2-runtime delete ecosystem.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/cli": "7.24.1",
    "@babel/core": "7.24.4",
    "@babel/node": "7.23.9",
    "@babel/preset-env": "7.24.4",
    "@babel/preset-typescript": "7.24.1",
    "@eslint/eslintrc": "3.0.2",
    "@eslint/js": "9.1.1",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.17",
    "@types/apicache": "1.6.6",
    "@types/cors": "2.8.17",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "^20.12.7",
    "@types/node-cache": "4.2.5",
    "@types/require-dir": "^1.0.4",
    "@types/swagger-jsdoc": "6.0.4",
    "@types/swagger-ui-express": "4.1.6",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "babel-plugin-module-resolver": "5.0.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "43.0.1",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "globals": "15.0.0",
    "prettier": "3.2.5",
    "rimraf": "5.0.5",
    "ts-node-dev": "2.0.0",
    "tsc-watch": "6.2.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.5.2",
    "unplugin-swc": "1.4.5"
  },
  "dependencies": {
    "@databases/sqlite": "^4.0.2",
    "@logtail/node": "0.4.21",
    "@logtail/winston": "0.4.21",
    "axios": "^1.5.1",
    "better-sqlite3": "^12.5.0",
    "body-parser": "^1.20.2",
    "compression": "1.7.4",
    "cors": "2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "graphql": "16.8.1",
    "graphql-request": "6.1.0",
    "jsdom": "24.1.0",
    "knex": "^3.1.0",
    "knex-stringcase": "^1.5.5",
    "moment": "2.30.1",
    "morgan": "1.10.0",
    "mysql": "2.18.1",
    "node-cache": "5.1.2",
    "nodemailer": "6.9.14",
    "pm2": "^6.0.14",
    "prom-client": "^15.1.3",
    "qs": "6.12.1",
    "remove": "0.1.5",
    "require-dir": "1.2.0",
    "rpi-server": "file:",
    "sanitize-html": "2.13.0",
    "sqlite3": "^5.1.7",
    "stringcase": "4.3.1",
    "swagger-jsdoc": "6.2.8",
    "swagger-ui-express": "5.0.0",
    "uuid": "^10.0.0",
    "winston": "3.13.0",
    "zod": "3.23.6"
  }
}
